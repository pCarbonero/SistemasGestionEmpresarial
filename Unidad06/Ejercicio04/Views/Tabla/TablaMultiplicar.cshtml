@{
    ViewData["Title"] = "Tabla Multiplicar";
}

<div class="text-center">
    @Html.ActionLink("Volver", "Index", "Home")

    <table border="3" align="center" cellpadding="3">

        @for (int i = 1; i <= 12; i++)
        {
            <tr border = "3">
                @for (int j = 1; j <= 10; j++)
                {
                    <td style="border: 3px solid black">
                        @i * @j = @(i*j)
                        </td>
                }
            </tr>
        }

    </table>

</div>